{
  "@quasar/testing": {
    "harnesses": [
      "unit-jest",
      "e2e-cypress"
    ]
  },
  "@quasar/testing-unit-jest": {
    "babel": "babelrc",
    "options": [
      "scripts",
      "typescript",
      "SFC",
      "wallabyjs",
      "majestic"
    ]
  },
  "@quasar/testing-e2e-cypress": {
    "options": [
      "scripts",
      "typescript"
    ]
  },
  "@quasar/qenv": {
    "common_root_object": "none",
    "add_qenv_to_gitignore": true
  },
  "@quasar/typescript": {
    "codeUpdate": true,
    "componentStyle": "class",
    "vscode": true,
    "prettier": true
  }
}