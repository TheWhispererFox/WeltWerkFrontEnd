{
  "name": "worldforge",
  "version": "0.0.1",
  "description": "CMS for writers and worldbuilders",
  "productName": "WorldForge",
  "author": "TheWhisperer <houstonplanes@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic",
    "test:e2e": "cross-env E2E_TEST=true start-test 'quasar dev' http-get://localhost:8080 'cypress open'",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test 'quasar dev' http-get://localhost:8080 'cypress run'"
  },
  "dependencies": {
    "@quasar/cli": "^1.1.3",
    "@quasar/extras": "^1.9.14",
    "@vue/composition-api": "^0.6.4",
    "axios": "^0.21.1",
    "core-js": "^3.8.3",
    "quasar": "^1.15.1",
    "vue-i18n": "^8.22.4"
  },
  "devDependencies": {
    "@quasar/app": "^2.1.14",
    "@quasar/quasar-app-extension-testing": "^1.0.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^2.0.4",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.2.1",
    "@types/node": "^10.17.51",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-vue": "^6.1.2",
    "majestic": "^1.8.0",
    "wallaby-vue-compiler": "^1.0.3"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
